    "flexboxgrid": "^6.3.1",
    "jquery": "^3.6.0",
    "jquery-ui": "^1.12.1",
    "jquery-validation": "^1.19.1",
    "laravel-mix": "^4.1.2",
    "laravel-mix-merge-manifest": "^0.1.2",
    "lodash": "^4.17.21",
    "moment": "^2.24.0",
    "normalize.css": "^8.0.1",
    "ora": "^3.4.0",
    "postcss": "^8.3.7",
    "resolve-url-loader": "2.3.1",
